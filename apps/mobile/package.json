{
  "name": "@notify/mobile",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "dev": "npx expo start",
    "start": "npx expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "build": "echo 'Build step for mobile app - use expo export:web for web builds'",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prebuild": "expo prebuild --clean",
    "deploy:ios": "bundle exec fastlane ios beta",
    "deploy:android": "bundle exec fastlane android beta",
    "deploy:all": "bundle exec fastlane deploy_all",
    "setup:fastlane": "bundle install && bundle exec fastlane add_plugin firebase_app_distribution"
  },
  "dependencies": {
    "@expo/server": "^0.6.2",
    "@expo/vector-icons": "^14.1.0",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "^8.4.1",
    "@react-native-firebase/app": "^22.2.0",
    "@react-native-firebase/messaging": "^22.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.80.2",
    "expo": "~53.0.9",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-linking": "~7.1.5",
    "expo-random": "^14.0.1",
    "expo-router": "~5.0.6",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "~4.10.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "uuid": "^11.1.0",
    "zustand": "^5.0.5",
    "expo-build-properties": "~0.14.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/cli": "~0.24.0",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "turbo": "^2.5.4",
    "typescript": "~5.8.3"
  },
  "private": true
}
